{
  "name": "@mafalda/fsset",
  "version": "2.1.0",
  "description": "Filesystem-backed implementation of JavaScript `Set` class",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./lib/index.js",
    "./async.js": "./lib/async.js",
    "./waiting.js": "./lib/waiting.js"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepare": "npm run build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mafalda-SFU/FsSet.git"
  },
  "keywords": [
    "pidfile",
    "pid",
    "process",
    "lock",
    "filelock",
    "set",
    "filesystem",
    "file"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mafalda-SFU/FsSet/issues"
  },
  "homepage": "https://github.com/Mafalda-SFU/FsSet#readme",
  "dependencies": {
    "fs-ext": "^2.0.0",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "jest-cli": "^27.0.6",
    "tempy": "^1.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  }
}
