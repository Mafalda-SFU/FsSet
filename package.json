{
  "name": "@mafalda/fsset",
  "version": "2.2.0",
  "description": "Filesystem-backed implementation of JavaScript `Set` class",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/index.cjs"
    },
    "./async": {
      "import": "./async.js",
      "require": "./dist/async.cjs"
    },
    "./waiting": {
      "import": "./waiting.js",
      "require": "./dist/waiting.cjs"
    }
  },
  "scripts": {
    "docs": "cp README.md docs/ && jsdoc2md *.js > docs/API.md",
    "lint": "eslint .",
    "predocs": "mkdir -p docs/",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mafalda-SFU/FsSet.git"
  },
  "keywords": [
    "pidfile",
    "pid",
    "process",
    "lock",
    "filelock",
    "set",
    "filesystem",
    "file"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mafalda-SFU/FsSet/issues"
  },
  "homepage": "https://github.com/Mafalda-SFU/FsSet#readme",
  "dependencies": {
    "fs-ext": "^2.0.0",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@mafalda/eslint-config": "^0.0.1",
    "bytenode": "github:Mafalda-SFU/bytenode",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "jest-cli": "^29.3.1",
    "jsdoc-to-markdown": "^8.0.0",
    "tempy": "^3.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "*.js"
    ],
    "transform": {}
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
